# Config Server Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
CONFIG_SERVER_PORT=8888

# Security Credentials
CONFIG_SERVER_USERNAME=configuser
CONFIG_SERVER_PASSWORD=your-secure-password

# Eureka Configuration
EUREKA_URI=http://localhost:8761/eureka/

# Git Repository Configuration (for production)
# CONFIG_GIT_URI=https://github.com/your-org/config-repo
# CONFIG_GIT_BRANCH=main
# CONFIG_GIT_USERNAME=your-username
# CONFIG_GIT_PASSWORD=your-token

# Logging
LOG_PATH=./logs

# Environment
ENVIRONMENT=development
